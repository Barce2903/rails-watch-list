<h1>Lists</h1>
<div class="row">
  <% @lists.each do |list| %>
    <div class="col-3">
      <div class="card" style="width: 18rem; height: 150px;">
        <div class="card-custom">
          <%= link_to list.name, list_path(list), class: 'pb-2 text-white' %>
        </div>
      </div>
    </div>
  <% end %>
</div>
